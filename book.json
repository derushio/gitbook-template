
{
    "plugins": [
        "-search", "-lunr",
        "theme-api", "expandable-chapters", "page-toc-af",
        "include-codeblock", "include-csv", "ace",
        "uml", "graph"
    ],
    "pluginsConfig": {
        "theme-api": {
            "theme": "dark",
            "split": false
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "after-first",
            "showByDefault": true
        }
    },
    "styles": {
        "website": "src/styles/style.css"
    }
}
