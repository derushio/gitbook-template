
{
    "plugins": [
        "-search", "-lunr", "-sharing",
        "theme-api", "expandable-chapters", "page-toc-af",
        "include-codeblock", "include-csv", "ace",
        "uml", "graph"
    ],
    "pluginsConfig": {
        "theme-api": {
            "theme": "dark",
            "split": false
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4, .markdown-section h5, .markdown-section h6",
            "position": "after-first",
            "showByDefault": true
        }
    },
    "styles": {
        "website": "assets/styles/style.css"
    }
}
